  <div class="container">
    <table class="table table-striped table-dark table-hover caption-top">
      <caption style="color:orange; font-size:12pt; font-weight:bold; text-align:center">NebulaTraders</caption>
        <thead class="table-dark">
          <tr class="text-center">
            <th>Name</th>
            <th>Email</th>
            <th>User ID</th>
            <th>Status</th>
            <th>Created</th>
            <th>Updated</th>
            <th>Balance</th>
 
          </tr>
        </thead>
        <tbody>
          <% @user.each do |user| %>
            <tr class="text-center align-middle">
              <td class="text-left"><%= user.first_name %> <%= user.last_name %></td>
              <td><%= user.email %></td>
              <td><%= user.user_id %></td>
              <td><%= user.status %></td>
              <td><%= user.created_at %></td>
              <td><%= user.updated_at %></td>
              <td>$ <%= user.cash %></td>
            </tr>
          <% end %>
        </tbody>
      </table>

  <%= link_to 'Back', admins_path %>
</div>