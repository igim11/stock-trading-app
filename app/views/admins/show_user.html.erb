  <div class="container">
    <table class="table table-striped table-dark table-hover caption-top">
      <caption>Pending Sign-ups</caption>
        <thead class="table-dark">
          <tr>
            <th>Name</th>
            <th>Email</th>
            <th>User ID</th>
            <th>Registration Status</th>
            <th>Created At</th>
            <th>Updated At</th>
            <th>Actions</th>
          </tr>
        </thead>
        <tbody>
          <% users.each do |user| %>
            <tr>
              <td><%= user.first_name %> <%= user.last_name %></td>
              <td><%= user.email %></td>
              <td><%= user.user_id %></td>
              <td><%= user.status %></td>
              <td><%= user.created_at %></td>
              <td><%= user.updated_at %></td>
              <td><%= user.cash %></td>
            </tr>
          <% end %>
        </tbody>
      </table>

  <%= link_to 'Back', admins_path %>
</div>